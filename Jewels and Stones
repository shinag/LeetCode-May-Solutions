class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int count=0;
        for (int i=0; i<S.length() ;i++)       //for each stone
        {
            for(int j=0; j<J.length() ;j++)    //check if it is a jewel
            {
                if(S[i]==J[j])
                {
                    count ++;
                }
            }
        }return count;
    }
};
